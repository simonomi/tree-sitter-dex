{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "dex",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "event"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "event"
        }
      ]
    },
    "event": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_line"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        }
      ]
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "command"
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": "\\/\\/[^\\n]*"
    },
    "command": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "commandText"
          },
          {
            "type": "SYMBOL",
            "name": "argument"
          }
        ]
      }
    },
    "commandText": {
      "type": "PATTERN",
      "value": "[^<>\\n]+"
    },
    "argument": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "number"
            },
            {
              "type": "SYMBOL",
              "name": "prefix"
            },
            {
              "type": "SYMBOL",
              "name": "suffix"
            },
            {
              "type": "SYMBOL",
              "name": "argumentText"
            },
            {
              "type": "SYMBOL",
              "name": "flag"
            },
            {
              "type": "SYMBOL",
              "name": "vector"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "-?(0x)?[\\da-fA-F]+(\\.\\d+)?"
    },
    "argumentText": {
      "type": "PATTERN",
      "value": "[^<>\\n0-9]+"
    },
    "prefix": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "argumentText"
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "suffix": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "argumentText"
        }
      ]
    },
    "flag": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "vector": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "STRING",
          "value": ", "
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}